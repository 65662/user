/* pages/user/user.wxss */
.list {
  margin-bottom: 50rpx;
}
.header {
  padding: 0 35rpx;
  line-height: 75rpx;
}
.list .list_item {
  margin-top: 40rpx;
  display: flex;
  justify-content: space-between;
  line-height: 100rpx;
  border-bottom: 1px solid rgba(128, 128, 128, 0.424);
  padding: 0 50rpx;
  color: rgb(49, 49, 49);
  margin-left: 10rpx;
}
#addIcon{
  width: 50px;
  height: 50px;
  font-size: 40px;
  background-color: gainsboro;
  line-height: 45px;
  text-align: center;
  border-radius: 25px;
  position: fixed;
  top: 1000rpx;
  left: 600rpx;
}
